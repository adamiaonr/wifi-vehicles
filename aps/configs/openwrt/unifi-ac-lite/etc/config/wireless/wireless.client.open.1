config wifi-device  radio0
	option type mac80211
	option path	'platform/qca956x_wmac'
	option channel 'auto'
	option hwmode '11g'
	option country 'PT'
	option log_level '0'

config wifi-iface
	option device 'radio0'
	option network 'wwan'
	option mode 'sta'
	option ssid 'nikola-tesla'
	option bgscan 'simple:10:-65:60'
